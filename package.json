{
  "name": "my-react-router-app",
  "homepage": "https://Dithir.github.io/my-react-router-app/",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "preview": "vite preview",
    "nj": "touch ./docs/.nojekyll && touch ./.nojekyll",
    "hack-404":"cp ./docs/index.html ./docs/404.html",
    "unnest": "mv ./build/client/my-react-router-app/* ./build/client && rm -rf ./build/client/my-react-router-app",
    "build-clean": "rm -rf ./build && rm -rf ./docs",
    "build-cp":"cp -r ./build/client ./docs",
    "prepublish-prerender": "npm run build-clean && npm run build && npm run unnest && npm run build-cp && npm run nj  && npm run hack-404",
    "prepublish-no-prerender": "npm run build-clean && npm run build && npm run build-cp && npm run nj && npm run hack-404"
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "gh-pages": "^6.3.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
